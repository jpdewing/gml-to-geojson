{
  "name": "@permettez-moi-de-construire/gml-to-geojson",
  "version": "0.0.1-alpha",
  "description": "Translate gml geometries to geojson",
  "main": "dist/gml-to-geojson.js",
  "scripts": {
    "clean": "shx rm -rf dist",
    "ready": "shx mkdir -p dist",
    "compile": "npm run clean && npm run ready && webpack --env dev && webpack --env build",
    "clean:doc": "shx rm -rf doc",
    "ready:doc": "shx mkdir -p doc",
    "doc": "npm run clean:doc && npm run ready:doc && jsdoc2md src/**/*.js > doc/API.md",
    "build": "npm run compile && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require @babel/register --colors --recursive --require ./test/index.js ./test/**/*.js",
    "test:watch": "mocha --require @babel/register --colors -w --recursive --require ./test/index.js ./test/**/*.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:permettez-moi-de-construire/gml-to-geojson.git"
  },
  "keywords": [
    "es6",
    "library",
    "universal",
    "umd"
  ],
  "author": "Cyril CHAPON",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/permettez-moi-de-construire/gml-to-geojson/issues"
  },
  "homepage": "https://github.com/permettez-moi-de-construire/gml-to-geojson",
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.42",
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "babel-eslint": "8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lodash": "^4.17.5",
    "mocha": "^4.0.1",
    "shx": "^0.2.2",
    "sinon": "^4.4.6",
    "standard": "^11.0.1",
    "standard-loader": "^6.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {}
}
